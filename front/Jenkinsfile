#!/usr/bin/env groovy
pipeline {
    agent any
    stages {
        stage('Build') { 
            steps {
              dir('front') { 
                sh 'npm install' 
                sh 'npm run build' 
              }
           }
        stage('Deploy') { 
            steps {
              dir('front') { 
                sh 'npm run dev' 
              }
           }
       }
   }
}
